{
    "name": "microweber-deps/composer-shared-package-plugin",
    "description": "This composer plugin allows you to share selected packages between your projects with symlinks.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylvain Lorinet",
            "email": "sylvain.lorinet@gmail.com"
        },
        {
            "name": "Bozhidar Slaveykov",
            "email": "bobi@microweber.com"
        }
    ],
    "support": {
        "issues": "https://github.com/microweber-deps/composer-shared-package-plugin/issues"
    },
    "require": {
        "php": ">=7.3.0",
        "composer-plugin-api": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "composer/composer": "dev-master",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-0": {
            "LEtudiant\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "Test\\Unit\\LEtudiant\\Composer\\": "tests/unit"
    },
    "extra": {
        "class": "LEtudiant\\Composer\\SharedPackagePlugin"
    }
}
